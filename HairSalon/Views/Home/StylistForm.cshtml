@{
  Layout = "_Layout";
}
<h1>Add Stylist</h1>
<br></br>
<form action="/index/new" method="post">
  <div class="form-group">
    <label class= "stylist-name" for="stylist-name">Name:</label>
       <input type="text" name="stylist-name" id="stylist-name">
       <label class="control-label" for="date">Start Date</label>
       <input class="start-date" id="date" name="start-date" placeholder="MM/DD/YYY" type="text"/>
  </div>
  <div class="form-group">
    <button class="btn" type="submit">Submit</button>
  </div>
  <a href="/">Go Back</a>
